  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1"
